; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="SmartCubs"
run/main_scene="res://scene's/menu/level_selector_screen/level_selector.tscn"
config/icon="res://icon.png"

[autoload]

SceanTransition="*res://scene's/scean_trans/scean_transition.tscn"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=640
window/size/test_height=360
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/shadows/filter_mode.mobile=1
quality/filters/anisotropic_filter_level=5
quality/filters/msaa=3
environment/default_environment="res://default_env.tres"
